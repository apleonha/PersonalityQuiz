//This method is called after the user completes all the questions to calculate type
//this method calls the display method for the user's determined type
void calculateType(){
		int type = 0;
		Int[] array = scoreArray();
		
		for (int i = 0; i < array.length(); i++){
			int current = array[i];
  			if (current > type){
       			type = current;
       		}
       	
       	//call display method for calculated type
       	switch (type){
			case 1: 
				display1();
			case 2:
				display2();
				break;
			case 3:
				display3();
				break;
			case 4:
				display4();
				break;
			case 5:
				display5();
				break;
			case 6:
				display6();
				break;
			case 7:
				display7();
				break;
			case 8:
				display8();
				break;
			case 9:
				display9();
				break;
			}
	}

}
